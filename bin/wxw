#!/usr/bin/env node

var args, out, wc;

wc = require('../js/wc');

args = process.argv.slice(2);

out = wc.exec.apply(null, args);

switch (args[0]) {
    case 'launch':
    case 'raise':
    case 'focus':
    case 'hook':
        process.stdout.write("\n");
        break;
    default:
        process.stdout.write(out);
}
